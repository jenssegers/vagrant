        "php",
        "php::module_mcrypt",
        "php::module_mysql",
        "php::module_sqlite3",
        "php::module_pgsql",
        "php::module_memcache",
        "php::module_apc",
        "php::module_curl",
        "php::module_gd",
        "php::module_igbinary",
        "php::module_redis",
        "php::module_blitz",
        "composer",
        "composer::self_update",
        "nodejs",
        "tools::bower",
        "tools::grunt",
        "tools::gulp",
        "tools::phpunit",
